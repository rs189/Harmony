{
    "domains": [
        "win10-games",
        "win10-vr",
        "win10-gpu-software"
    ],
    "looking-glass-gdk-backend": "wayland",
    "looking-glass-path": "~/Git/LookingGlass/client/build/looking-glass-client",
    "looking-glass-args": [
        "win:quickSplash=yes",
        "wayland:warpSupport=no",
        "audio:micShowIndicator=no", 
        "audio:micDefault=allow",
        "spice:captureOnStart=no",
        "input:captureOnFocus=yes",
        "input:grabKeyboard=no",
        "egl:noSwapDamage=yes"
    ],
    "spice-port": 5905,
    "harmony-port": 5000
}